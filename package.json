    "joi": "^14.3.0"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "google-cloud-gui": "^0.2.2",
    "google-datastore-emulator": "^2.1.0",
    "jest": "^23.6.0",
    "polymer-cli": "^1.9.1",
    "polymer-modulizer": "^0.4.1",
    "supertest": "^3.3.0",
    "wct-browser-legacy": "^1.0.1"
  },
  "resolutions": {
    "inherits": "2.0.3",
